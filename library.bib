author = {van Elst, Ludger Tebartz and Biscaldi-Sch{\"{a}}fer, Monica and Riedel, Andreas},
doi = {10.1007/s15005-014-0005-5},
journal = {InFo Neurologie {\&} Psychiatrie},
number = {4},
